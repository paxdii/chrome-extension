{
    "manifest_version": 3,
    "name": "Dream Runner",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "./content/icon_16.png",
        "48": "./content/icon_48.png",
        "128": "./content/icon_128.png"
      }
    },
    "icons": {
        "16": "./content/icon_16.png",
        "48": "./content/icon_48.png",
        "128": "./content/icon_128.png"
    },
    "permissions": [],
    "web_accessible_resources": [
      {
        "resources": ["p5.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  