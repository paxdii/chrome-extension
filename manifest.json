{
    "manifest_version": 2,
    "name": "Meine Erweiterung",
    "version": "1.0",
    "description": "Eine einfache Chrome-Erweiterung",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["activeTab"]
  }
  